{
  "name": "@fe6/biu-deps-babel",
  "version": "0.0.0",
  "description": "@fe6/biu-deps-babel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "compiled"
  ],
  "scripts": {
    "build": "pnpm tsc",
    "build:deps": "pnpm esno ../../scripts/bundle-deps.ts",
    "start": "pnpm build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fe6/biu.git",
    "directory": "packages/deps-babel"
  },
  "authors": [
    "tainyi <649076408@qq.com> (https://github.com/iq9891)"
  ],
  "license": "MIT",
  "bugs": "https://github.com/fe6/biu/issues",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/code-frame": "7.16.7",
    "@babel/core": "7.17.7",
    "@babel/generator": "7.17.7",
    "@babel/helper-module-imports": "7.16.7",
    "@babel/parser": "7.17.7",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.2",
    "@babel/plugin-proposal-do-expressions": "7.16.7",
    "@babel/plugin-proposal-export-default-from": "7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "7.16.7",
    "@babel/plugin-proposal-function-bind": "7.16.7",
    "@babel/plugin-proposal-partial-application": "7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "7.17.6",
    "@babel/plugin-proposal-record-and-tuple": "7.16.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/register": "7.17.7",
    "@babel/template": "7.16.7",
    "@babel/traverse": "7.17.3",
    "@babel/types": "7.17.0",
    "@types/babel__code-frame": "7.0.3",
    "@types/babel__generator": "7.6.4",
    "@types/babel__parser": "7.1.1",
    "@types/babel__template": "7.4.1",
    "@types/babel__traverse": "7.14.2",
    "@vue/babel-plugin-jsx": "1.1.1",
    "babel-loader": "8.2.4"
  },
  "compiledConfig": {
    "deps": [
      "./bundles/babel/bundle",
      "babel-loader",
      "@vue/babel-plugin-jsx"
    ],
    "externals": {
      "babel-loader": "$$LOCAL",
      "@vue/babel-plugin-jsx": "$$LOCAL"
    },
    "extraDtsDeps": [],
    "noMinify": [
      "./bundles/babel/bundle",
      "babel-loader"
    ],
    "excludeDtsDeps": [
      "@babel/code-frame",
      "@babel/core",
      "@babel/generator",
      "@babel/parser",
      "@babel/template",
      "@babel/traverse",
      "@babel/types",
      "babel-loader",
      "@vue/babel-plugin-jsx"
    ],
    "extraDtsExternals": []
  }
}
